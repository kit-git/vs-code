// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+`",
    "command": "-workbench.action.selectTheme"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+`",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  },
  {
    "key": "shift+alt+f",
    "command": "-editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
  },
  {
    "key": "ctrl+`",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+cmd+l",
    "command": "-editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+cmd+b",
    "command": "-editor.action.goToImplementation",
    "when": "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "ctrl+cmd+i",
    "command": "-workbench.action.chat.open"
  },
  {
    "key": "cmd+3",
    "command": "workbench.action.chat.openEditSession",
    "when": "chatEditingParticipantRegistered && view != 'workbench.panel.chat.view.edits'"
  },
  {
    "key": "shift+cmd+i",
    "command": "-workbench.action.chat.openEditSession",
    "when": "chatEditingParticipantRegistered && view != 'workbench.panel.chat.view.edits'"
  },
  {
    "key": "cmd+4",
    "command": "workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled && activeViewlet != 'workbench.view.debug'"
  },
  {
    "key": "cmd+5",
    "command": "-workbench.view.debug",
    "when": "viewContainer.workbench.view.debug.enabled && activeViewlet != 'workbench.view.debug'"
  },
  {
    "key": "f5",
    "command": "workbench.action.debug.restart",
    "when": "inDebugMode"
  },
  {
    "key": "shift+cmd+f5",
    "command": "-workbench.action.debug.restart",
    "when": "inDebugMode"
  },
  {
    "key": "shift+enter",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
      "text": "\\\r\n"
    },
    "when": "terminalFocus"
  },
  {
    "key": "cmd+2",
    "command": "workbench.action.chat.toggle"
  },
  {
    "key": "cmd+2",
    "command": "-workbench.action.focusSecondEditorGroup"
  },
  {
    "key": "cmd+1",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "alt+cmd+b",
    "command": "-workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "cmd+1",
    "command": "-workbench.action.focusFirstEditorGroup"
  },
  {
    "key": "cmd+1",
    "command": "-workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled && !explorerViewletFocus"
  },
  {
    "key": "cmd+1",
    "command": "-workbench.action.toggleSidebarVisibility",
    "when": "explorerViewletFocus"
  },
  {
    "key": "cmd+3",
    "command": "workbench.action.togglePanel"
  },
  {
    "key": "alt+cmd+tab",
    "command": "workbench.action.nextPanelView"
  }
]
